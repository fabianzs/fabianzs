@model IEnumerable<TodoApp.Models.Todo>
<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>Getting Started: Serving Web Content</title>
</head>
<body>
    <h1>Todos for today:</h1>
    <ul>
        @foreach (var todo in Model)
        {
            <li>@todo.Title</li>
        }
        </ul>
</body>
</html>